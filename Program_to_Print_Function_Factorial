import java.util.*;
public class FunFactorial{
    public static int Factorial (int n){
        int fact = 1;
        for(int i = 1;i<=n;i++){
            fact*=i;
        }
        return fact;
    }
    public static void main(String args[]){
        int fact = 1;
        Scanner sc = new Scanner (System.in);
        System.out.println("Enter the number ");
        int n = sc.nextInt();
        int fac = Factorial(n);
        System.out.println("factorial = "+fac);
    }
}
