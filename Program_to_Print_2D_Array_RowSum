public class SumSingleRTwoD{
    public static int SecondRowSum (int matrix[][]){
        int row = 1;
        int sum = 0;
        for(int j=0;j<matrix[0].length;j++){
            sum+=matrix[row][j];
        }
        return sum;
    }
    public static void main (String args[]){
        int matrix [][] = {{1,4,9},
                           {11,4,3},
                           {2,2,3}};
                          System.out.println(SecondRowSum(matrix));
        
    }
}
