public class LargestOfThree{
    public static void main(String args[]){
        int a = 1;
        int b = 3;
        int c = 6;
        if( (a>=b) && (a>=c) ){
            System.out.println("Largest is A = "+ a);
        }
        else if (b>=c){
            System.out.println("Largest is B = "+ b);
        }
        else {
            System.out.println("Largest is c =" + c);
        }
    }
}
