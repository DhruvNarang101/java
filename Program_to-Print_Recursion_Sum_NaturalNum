public class RecursionSumNatural{
    public static int Calsum (int n){
        if(n==1){
            return 1;
        }
        int sm1 = Calsum(n-1);
        int s = n+sm1;
        return s;
    }
    public static void main (String args[]){
        int n = 5;
        System.out.println(Calsum(n));
    }
}
