public class RecursionFibonacci {
    public static int fib(int n){
        if(n==1|| n==0){
            return n;
        }
        int fnm1 = fib(n-1);
        int fnm2 = fib(n-2);
        int f = fnm1 + fnm2;
        return f;
    }
    public static void main (String args[]){
        int n =4;
        System.out.println(fib(n));
    }
}
